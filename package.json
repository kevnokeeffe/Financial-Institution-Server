{
  "name": "my-bank",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./bin/www",
    "start:dist": "node ./dist/bin/www",
    "start": "cross-env NODE_ENV=dev node dist/bin/www",
    "test": "cross-env NODE_ENV=test mocha ",
    "test:report": "nyc mocha",
    "clean": "rimraf ./dist && mkdir dist",
    "compile:models": "babel ./models --out-dir dist/models",
    "compile:controllers":"babel ./controllers --out-dir dist/controllers",
    "compile:middleware": "babel ./middleware --out-dir dist/middleware",
    "compile:services": "babel ./services --out-dir dist/services",
    "compile:routes": "babel ./routes --out-dir dist/routes",
    "compile:app": "babel app.js --out-dir dist/",
    "compile:bin": "babel ./bin --out-dir dist/bin",
    "compile:db": "babel ./db --out-dir dist/db",
    "compile:copy": "cp -r node_modules/ dist/node_modules && cp -r views/ dist/views && cp -r public/ dist/public",
    "compile:all": "npm-run-all compile:db compile:models compile:routes compile:middleware compile:services compile:app compile:copy",
    "prebuild": "npm-run-all test clean",
    "build:release": "npm-run-all test clean compile:all",
    "postbuild": "npm run start:dist",
    "postinstall": "rimraf ./dist && npm run compile:all"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^5.9.0",
    "validatorjs": "^3.18.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "chai": "^4.2.0",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "http": "0.0.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^7.0.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2"
  }
}
